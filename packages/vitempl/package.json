{
  "name": "vitempl",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Stepan Sotnikov",
  "description": "Template engine with Vite",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "homepage": "https://github.com/sotnikovse/vitempl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sotnikovse/vitempl.git"
  },
  "bugs": {
    "url": "https://github.com/sotnikovse/vitempl/issues"
  },
  "keywords": [
    "frontend",
    "hmr",
    "dev-server",
    "build-tool",
    "vite"
  ],
  "packageManager": "pnpm@8.9.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.3.7",
    "vite": "^4.4.5"
  },
  "devDependencies": {
    "rollup": "^4.1.4"
  }
}
